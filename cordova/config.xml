<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gmail.appsdmtech.psichelp" version="0.0.57" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PsicHelp</name>
    <description>Aplicativo para ajudar pessoas buscando tratamento psicoterapÃªutico em Salvador</description>
    <author email="evandrojr@gmail.com" href="http://psichelp.com.br">Equipe PsicHelp</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
